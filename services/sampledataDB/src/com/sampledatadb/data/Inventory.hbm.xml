<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.sampledatadb.data.Inventory" table="inventory" dynamic-insert="false" dynamic-update="false">
        <id name="inventoryId" type="integer">
            <column name="INVENTORY_ID"/>
            <generator class="identity"/>
        </id>
        <many-to-one name="store" class="com.sampledatadb.data.Store" cascade="none">
            <column name="STORE_ID" not-null="true"/>
        </many-to-one>
        <many-to-one name="film" class="com.sampledatadb.data.Film" cascade="none">
            <column name="FILM_ID" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>